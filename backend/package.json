{
  "name": "@roadmap/backend",
  "version": "1.0.0",
  "description": "Roadmap Node Prototype Backend",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon",
    "prestart:prod": "tsc",
    "build:prod": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "start:prod": "cross-env NODE_ENV=production node dist/backend/src/server.js",
    "test": "cross-env NODE_ENV=test jest --config=jest.json --coverage --runInBand --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config=jest.json",
    "test:coverage": "cross-env NODE_ENV=test jest --config=jest.json --coverage --coverageDirectory=coverage",
    "e2e": "cross-env NODE_ENV=test jest --config=e2e/jest-e2e.json",
    "e2e:watch": "cross-env NODE_ENV=test jest --watch --config=e2e/jest-e2e.json"
  },
  "dependencies": {
