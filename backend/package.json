    "@nestjs/microservices": "4.6.4",
    "@nestjs/swagger": "1.1.4",
    "@nestjs/testing": "4.6.1",
    "@nestjs/websockets": "4.5.8",
    "@roadmap/shared": "1.0.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "1.16.8",
    "@types/bunyan": "1.8.4",
    "@types/cors": "2.8.3",
    "@types/express": "4.11.1",
    "@types/jest": "21.1.10",
    "@types/mssql": "4.0.7",
